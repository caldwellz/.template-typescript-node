{
  "name": "",
  "version": "0.0.1",
  "description": "",
  "author": "Zach Caldwell",
  "license": "MPL-2.0",
  "private": true,
  "main": "./dist/index.js",
  "engines": {
    "node": ">=12.9.0"
  },
  "scripts": {
    "build": "tsc --build",
    "coverage": "nyc npm run test",
    "dev": "nodemon src/index.ts",
    "format": "prettier --write '**/*' --ignore-unknown",
    "lint": "npm run lint-json && npm run lint-ts",
    "lint-json": "eslint --cache --fix --no-eslintrc -c .eslintrc-json.json --ext .json .",
    "lint-ts": "tsc --noEmit && eslint --cache --fix --ext .cjs,.js,.jsx,.mjs,.ts,.tsx .",
    "prepare": "husky install",
    "prod": "NODE_ENV=production npm run start",
    "start": "npm run build && node ./dist/index.js",
    "test": "NODE_ENV=testing mocha -r ts-node/register --check-leaks --parallel src/**/*.test.ts"
  },
  "devDependencies": {
    "@types/chai": "^4.3.0",
    "@types/mocha": "^9.1.0",
    "@types/sinon": "^10.0.11",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.12.1",
    "@typescript-eslint/parser": "^5.12.1",
    "chai": "^4.3.6",
    "eslint": "^8.9.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-json": "^3.1.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-unicorn": "^41.0.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.5",
    "mocha": "^9.2.1",
    "nodemon": "^2.0.15",
    "nyc": "^15.1.0",
    "prettier": "^2.5.1",
    "sinon": "^13.0.1",
    "supertest": "^6.2.2",
    "ts-node": "^10.5.0"
  },
  "dependencies": {
    "@types/config": "^0.0.41",
    "config": "^3.3.7",
    "typescript": "^4.5.5"
  }
}
